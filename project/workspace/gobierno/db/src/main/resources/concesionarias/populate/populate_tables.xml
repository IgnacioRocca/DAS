    <!-- ******************************** -->
    <changeSet id="Populate table config_tecnologica" author="DAS-Team" runAlways="true" runOnChange="true">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="config_tecnologica" />
        </preConditions>
